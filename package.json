{
  "name": "n8n-nodes-dragos",
  "version": "0.1.6",
  "description": "n8n nodes for the Dragos OT/ICS cybersecurity platform",
  "keywords": [
    "n8n-community-node-package",
    "dragos",
    "ics",
    "ot",
    "cybersecurity",
    "industrial"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Jake"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DragosApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Dragos/Dragos.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "gulp": "^5.0.1",
    "n8n-workflow": "^1.17.0",
    "prettier": "^3.1.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
